<% form_for @car do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :make %><br />
    <%= f.text_field :make %>
  </p>
  <p>
    <%= f.label :model %><br />
    <%= f.text_field :model %>
  </p>
  <p>
    <%= f.label :model_year %><br />
    <%= f.text_field :model_year %>
  </p>
  <p>
    <%= f.label :manufacture_year %><br />
    <%= f.text_field :manufacture_year %>
  </p>
  <p>
    <%= f.label :board %><br />
    <%= f.text_field :board %>
  </p>
  <p>
    <%= f.label :color %><br />
    <%= f.text_field :color %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
